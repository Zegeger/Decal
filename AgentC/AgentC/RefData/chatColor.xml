    <Reference name="ChatColorRules">
      <Value name="RuleXML" type="XML">
        <Rules>
          <Rule ReturnValue="General_Chat">
            <All>
              <StringTest Index="1" Match="[General] " Type="SW" />
              <IntegerTest Index="0" Match="%%ChatClasses%General%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Trade_Chat">
            <All>
              <StringTest Index="1" Match="[Trade] " Type="SW" />
              <IntegerTest Index="0" Match="%%ChatClasses%Trade%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Spellwords_Out">
            <All>
              <StringTest Index="1" Match="You say," Type="SW" />
              <IntegerTest Index="0" Match="%%ChatClasses%Spell Words%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Spellwords_In">
            <All>
              <StringTest Index="1" Match=".*says,.*" Type="SW" />
              <IntegerTest Index="0" Match="%%ChatClasses%Spell Words%%" Type="RX" />
            </All>
          </Rule>
          <Rule ReturnValue="Resists">
            <All>
              <StringTest Index="1" Match="You resist" Type="SW" />
              <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Spellcasting_Out">
            <Any>
              <All>
                <StringTest Index="1" Match="You say," Type="SW" />
                <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
              </All>
              <All>
                <StringTest Index="1" Match="Your spell fizzled" Type="SW" />
                <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
              </All>
            </Any>
          </Rule>
          <Rule ReturnValue="Spellcasting_In">
            <All>
              <StringTest Index="1" Match=".*says,.*" Type="RX" />
              <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Your_Magic_Combat">
            <All>
              <StringTest Index="1" Match="You .* for .* with .*" Type="RX" />
              <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Enemy_Magic_Combat">
            <All>
              <StringTest Index="1" Match=".* you for .* with .*" Type="RX" />
              <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Comp_Burn">
            <All>
              <StringTest Index="1" Match="The spell consumed" Type="SW" />
              <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Critical_Hits_Out">
            <Any>
              <All>
                <StringTest Index="1" Match="Critical hit!.*You.*" Type="RX" />
                <IntegerTest Index="0" Match="%%ChatClasses%Attack Out%%" Type="EQ" />
              </All>
              <All>
                <StringTest Index="1" Match="Critical hit!.*You.*" Type="RX" />
                <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
              </All>
            </Any>
          </Rule>
          <Rule ReturnValue="Critical_Hits_In">
            <Any>
              <All>
                <StringTest Index="1" Match="Critical hit!" Type="SW" />
                <IntegerTest Index="0" Match="%%ChatClasses%Attack In%%" Type="EQ" />
              </All>
              <All>
                <StringTest Index="1" Match="Critical hit!" Type="SW" />
                <IntegerTest Index="0" Match="%%ChatClasses%Spells%%" Type="EQ" />
              </All>
            </Any>
          </Rule>
          <Rule ReturnValue="Your_Evades">
            <All>
              <StringTest Index="1" Match="You evaded" Type="SW" />
              <IntegerTest Index="0" Match="%%ChatClasses%Attack In%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="Enemy_Evades">
            <All>
              <StringTest Index="1" Match=".*evaded your attack." Type="RX" />
              <IntegerTest Index="0" Match="%%ChatClasses%Attack Out%%" Type="EQ" />
            </All>
          </Rule>
          <Rule ReturnValue="No_Damage">
            <Any>
              <All>
                <StringTest Index="1" Match=".*for 0 points.*" Type="RX" />
                <IntegerTest Index="0" Match="%%ChatClasses%Attack In%%" Type="EQ" />
